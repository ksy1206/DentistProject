	<link href="/admin/css/daterangepicker.min.css?$version" rel="stylesheet">
</head>

<body class="fixed-nav sticky-footer bg-dark" id="page-top">

## Navigation
#parse("dentist/common/nav.vm")

<div class="content-wrapper">
	<div class="container-fluid">

		## 회원정보 페이지 Header
		#parse("dentist/common/memberHeader.vm")

		## 타입에 따른 페이지 Details
		#if($type == "info")
			#parse("dentist/member/info.vm")
		#elseif($type == "imgUpload")
			#parse("dentist/member/imgUpload.vm")
		#elseif($type == "schedule")
		
		#elseif($type == "deviceInfo")
		
		#elseif($type == "qa")
			#parse("dentist/member/qa.vm")
		#end

	</div>

	
	## Footer
	#parse("dentist/common/footer.vm")
